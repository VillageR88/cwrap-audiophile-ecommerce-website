      "children": [
        {
          "element": "cwrap-template",
          "text": "cwrapTemplate[sharedNavbar(rightMargin=100px)]"
        },
        {
          "element": "div",
          "mediaQueries": [
            {
              "query": "max-width: 1024px",
              "style": "align-items: center; text-align: center;"
            }
          ],
          "style": "display: flex; flex-direction: column; margin: 128px auto 0 auto; max-width: 1110px;",
          "children": [
            {
              "element": "span",
              "style": "color: rgba(255,255,255,0.4964); font-size: 14px; letter-spacing: 10px; max-width: 210px;",
              "text": "NEW PRODUCT"
            },
            {
              "element": "section",
              "mediaQueries": [
                {
                  "query": "max-width: 1024px",
                  "style": "align-items: center;"
                }
              ],
              "style": "display: flex; flex-direction: column; gap: 24px; margin-top: 24px; max-width: 400px;",
              "children": [
                {
                  "element": "h1",
                  "style": ";",
                  "text": "XX99 MARK II\nHEADPHONES"
                },
                {
                  "element": "p",
                  "style": "color: rgba(255,255,255,0.75); max-width: 350px;",
                  "text": "Experience natural, lifelike audio and exceptional build quality made for the passionate music enthusiast."
                }
              ]
            },
            {
              "element": "a",
              "attributes": {
                "class": "button1",
                "href": "headphones/xx99-mark-two-headphones"
              },
              "style": "background-color: #D87D4A; margin-top: 40px; transition: 150ms;",
              "extend": [
                {
                  "extension": ":hover",
                  "style": "background-color: #FBAF85;"
                }
              ],
              "text": "SEE PRODUCT"
            }
          ]
        }
      ]
    },
    {
      "element": "main",
