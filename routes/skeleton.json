      {
        "name": "viewport",
        "content": "width=device-width, initial-scale=1.0"
      },
      {
        "charset": "UTF-8"
      },
      {
        "name": "description",
        "content": "New Candy Wrapper project."
      },
      {
        "name": "keywords",
        "content": ""
      }
    ]
  },
  "classroom": [],
  "element": "body",
