              "style": "background-color: #F1F1F1; max-width: 540px; min-height: 320px; width: 100%; border-radius: 8px; background-image: url(\"static/images/home/desktop/image-earphones-yx1.jpg\");"
            },
            {
              "element": "div",
              "style": "background-color: #F1F1F1; width: 100%; max-width: 540px; border-radius: 8px; display: flex;",
              "children": [
                {
                  "element": "cwrap-template",
                  "text": "cwrapTemplate[sectionHome1(title=YX1 EARPHONES,link=earphones/yx1-earphones)]"
                }
              ]
            }
          ]
        },
        {
          "element": "cwrap-template",
          "text": "cwrapTemplate[divBottomHero]"
        }
      ]
    },
    {
      "element": "cwrap-template",
      "text": "cwrapTemplate[footerUniversal]"
    },
    {
      "element": "script",
      "attributes": {
        "src": "../static/scripts/cart.js",
        "type": "module"
      }
    }
