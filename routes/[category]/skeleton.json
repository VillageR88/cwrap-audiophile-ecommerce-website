{
  "routes": [
    {
      "route": "headphones"
    },
    {
      "route": "speakers"
    },
    {
      "route": "earphones"
    }
  ],
  "head": {
    "meta": [
      {
        "name": "viewport",
        "content": "width=device-width, initial-scale=1.0"
      },
      {
        "charset": "UTF-8"
      },
      {
        "name": "description",
        "content": "New Candy Wrapper project."
      },
      {
        "name": "keywords",
        "content": ""
      }
    ]
  },
  "fonts": [],
  "classroom": [
    {
      "name": "category-container",
      "type": "id",
      "style": "display: flex; flex-direction: column; gap: 160px; margin-block: 160px; margin-inline: auto; max-width: 1110px;"
    },
    {
      "name": "category-container-wrapper",
      "type": "class",
      "mediaQueries": [
        {
          "query": "max-width: 1200px",
          "style": "max-width: 1050px;"
        },
        {
          "query": "max-width: 960px",
          "style": "flex-direction: column;"
        }
      ],
      "style": "align-items: center; display: flex; gap: 24px; justify-content: space-between; margin-inline: auto; width: 100%;"
    },
    {
      "name": "category-container > div:nth-of-type(even)",
      "type": "id",
      "mediaQueries": [
        {
          "query": "max-width: 960px",
          "style": "flex-direction: column;"
        }
      ],
      "style": "flex-direction: row-reverse;"
    },
    {
      "name": "category-container-picture",
      "type": "class",
      "style": "display: flex; justify-content: center; width: 50%;",
      "mediaQueries": [
        {
          "query": "max-width: 960px",
          "style": "width: 100%;"
        }
      ]
    },
    {
      "name": "category-container-image",
      "type": "class",
      "mediaQueries": [
        {
          "query": "max-width: 960px",
          "style": "width: 100%;"
        }
      ],
      "style": "border-radius: 8px; display: flex; max-width: 540px; width: 100%;"
    },
    {
      "name": "category-container-div",
      "type": "class",
      "mediaQueries": [
        {
          "query": "max-width: 960px",
          "style": "align-items: center; text-align: center;"
        }
      ],
      "style": "display: flex; flex-direction: column; max-width: 445px;"
    },
    {
      "name": "category-container-div-new",
      "type": "class",
      "style": "color: #D87D4A; font-size: 14px; letter-spacing: 14px; margin-bottom: 16px;"
    },
    {
      "name": "category-container-div-section",
      "type": "class",
      "style": "display: flex; flex-direction: column; gap: 32px;"
    },
    {
      "name": "category-container-div-section-title",
      "type": "class",
      "style": "font-size: 40px; font-weight: bold; letter-spacing: 1.43px; line-height: 44px;"
    },
    {
      "name": "category-container-div-section-description",
      "type": "class",
      "style": "color: rgba(0,0,0,0.5);"
    },
    {
      "name": "category-container-div-section-button",
      "type": "class",
      "style": "background-color: #D87D4A; margin-top: 40px; transition: 150ms;"
    },
    {
      "name": "category-container-div-section-button:hover",
      "type": "class",
      "style": "background-color: #FBAF85;"
    }
  ],
  "element": "cwrap-template",
  "omit": [
    "fullMask",
    "pictureHero"
  ],
  "text": "cwrapTemplate[bodyCategory(prefix=../,routeTitle=cwrapRoutes[HEADPHONES,SPEAKERS,EARPHONES])]",
  "passover": [
    {
      "element": "div",
      "attributes": {
        "id": "category-container"
      }
    }
  ]
}