{
  "head": {
    "meta": [
      {
        "name": "viewport",
        "content": "width=device-width, initial-scale=1.0"
      },
      {
        "charset": "UTF-8"
      },
      {
        "name": "description",
        "content": "New Candy Wrapper project."
      },
      {
        "name": "keywords",
        "content": ""
      }
    ]
  },
  "classroom": [
    {
      "name": "h2-checkout",
      "type": "class",
      "style": "color: #D87D4A; font-size: 13px; line-height: 25px; letter-spacing: 0.93px;"
    },
    {
      "name": "input-checkout",
      "type": "class",
      "style": "font-family: Manrope; min-height: 56px; border-radius: 8px; border: 1px solid #CFCFCF; outline: none; padding-inline: 24px; font-weight: bold; font-size: 14px; letter-spacing: -0.25px; box-sizing: border-box; transition: 150ms;"
    },
    {
      "name": "input-checkout::placeholder",
      "type": "class",
      "style": "color: rgba(0,0,0,0.4);"
    },
    {
      "name": "input-checkout:focus",
      "type": "class",
      "style": "border-color: #D87D4A"
    },
    {
      "name": "input-checkout:hover",
      "type": "class",
      "style": "border-color: #D87D4A"
    },
    {
      "name": "label1",
      "type": "class",
      "style": "font-weight: bold; font-size: 12px; letter-spacing: -0.21px;"
    },
    {
      "name": "checkout-summary-container-list-item-count",
      "type": "class",
      "style": "font-weight: bold; font-size: 15px; line-height: 25px; color: rgba(0,0,0,0.5); align-self: start; margin-top: 7px;"
    },
    {
      "name": "checkout-summary-container-div",
      "type": "class",
      "style": "display: flex; align-items: center; justify-content: space-between;"
    },
    {
      "name": "checkout-summary-container-h3",
      "type": "class",
      "style": "font-size: 15px; font-weight: 500; color: rgba(0,0,0,0.5); line-height: 25px; margin: 0;"
    },
    {
      "name": "checkout-summary-container-h3-p",
      "type": "class",
      "style": "font-size: 18px; font-weight: bold;"
    }
  ],
  "element": "cwrap-template",
  "omit": ["ulNavigation2", "divBottomHero"],
  "text": "cwrapTemplate[bodyCategory(omit=cwrapOmit,routeTitle=cwrapOmit,backgroundColor=#F1F1F1,scriptName=checkout.js)]",
  "passover": [
    {
      "element": "div",
      "style": "max-width: 1110px; margin-inline: auto; margin-top: 79px; margin-bottom: 141px; box-sizing: border-box;",
      "children": [
        {
          "element": "a",
          "style": "margin-top: 79px; font-weight: 500; font-size: 15px; color: rgba(0,0,0,0.5); line-height: 25px; transition: 150ms; text-decoration: none;",
          "attributes": {
            "href": "../"
          },
          "text": "Go Back",
          "extend": [
            {
              "extension": ":hover",
              "style": "color: #D87D4A;"
            }
          ]
        },
        {
          "element": "form",
          "attributes": {
            "id": "checkout-form"
          },
          "style": "display: flex; justify-content: space-between; margin-top: 38px; gap: 30px;",
          "children": [
            {
              "element": "div",
              "style": "max-width: 730px; width: 100%; padding: 54px 48px 48px 48px; box-sizing: border-box; background-color: white; border-radius: 8px;",
              "children": [
                {
                  "element": "h1",
                  "style": "color: black; font-size: 32px; font-weight: bold; line-height: 36px; letter-spacing: 1.14px;",
                  "text": "CHECKOUT"
                },
                {
                  "element": "div",
                  "style": "display: flex; flex-direction: column; margin-top: 41px;",
                  "children": [
                    {
                      "element": "h2",
                      "attributes": {
                        "class": "h2-checkout"
                      },
                      "text": "BILLING DETAILS"
                    },
                    {
                      "element": "ul",
                      "style": "margin: 16px 0 0 0; padding: 0; display: grid; grid-template-areas: 'li1 li2' 'li3 _'; gap: 24px 16px;",
                      "blueprint": {
                        "element": "li",
                        "style": "display: flex; flex-direction: column; gap: 9px; grid-area: cwrapArray[li1,li2,li3]",
                        "count": 3,
                        "children": [
                          {
                            "element": "label",
                            "text": "cwrapArray[Name,Email Address,Phone Number]",
                            "attributes": {
                              "for": "cwrapArray[name,email,phone]",
                              "class": "label1"
                            }
                          },
                          {
                            "element": "input",
                            "attributes": {
                              "class": "input-checkout",
                              "pattern": "cwrapArray[cwrapGlobal[patternName]cwrapBreakcwrapGlobal[patternEmail]cwrapBreakcwrapGlobal[patternPhone]]",
                              "id": "cwrapArray[name,email,phone]",
                              "autocomplete": "cwrapArray[name,email,phone]",
                              "placeholder": "cwrapArray[Alexei Ward,alexei@mail.com,+1 202-555-0136]",
                              "required": ""
                            }
                          }
                        ]
                      }
                    }
                  ]
                },
                {
                  "element": "div",
                  "style": "display: flex; flex-direction: column; margin-top: 53px;",
                  "children": [
                    {
                      "element": "h2",
                      "attributes": {
                        "class": "h2-checkout"
                      },
                      "text": "SHIPPING INFO"
                    },
                    {
                      "element": "ul",
                      "style": "margin: 16px 0 0 0; padding: 0; display: grid; grid-template-areas: 'li1 li1' 'li2 li3' 'li4 _'; gap: 24px 16px;",
                      "blueprint": {
                        "element": "li",
                        "style": "display: flex; flex-direction: column; gap: 9px; grid-area: cwrapArray[li1,li2,li3,li4,li5]",
                        "count": 4,
                        "children": [
                          {
                            "element": "label",
                            "text": "cwrapArray[Address,ZIP Code,City, Country]",
                            "attributes": {
                              "for": "cwrapArray[street-address,postal-code,address-level2,address-level1]",
                              "class": "label1"
                            }
                          },
                          {
                            "element": "input",
                            "attributes": {
                              "class": "input-checkout",
                              "id": "cwrapArray[street-address,postal-code,address-level2,address-level1]",
                              "autocomplete": "cwrapArray[street-address,postal-code,address-level2,country-name]",
                              "placeholder": "cwrapArray[1137 Williams Avenue,10001,New York,United States]",
                              "required": ""
                            }
                          }
                        ]
                      }
                    }
                  ]
                },
                {
                  "element": "div",
                  "style": "display: flex; flex-direction: column; margin-top: 53px;",
                  "children": [
                    {
                      "element": "h2",
                      "attributes": {
                        "class": "h2-checkout"
                      },
                      "text": "PAYMENT DETAILS"
                    },
                    {
                      "element": "ul",
                      "style": "margin: 16px 0 0 0; padding: 0; display: grid; gap: 16px;",
                      "blueprint": {
                        "element": "li",
                        "style": "display: flex; flex-direction: row; gap: 9px; justify-content: space-between;",
                        "count": 2,
                        "children": [
                          {
                            "element": "h3",
                            "text": "cwrapArray[Payment Method,]",
                            "attributes": {
                              "for": "cwrapArray[Payment Method,]",
                              "class": "label1"
                            }
                          },
                          {
                            "element": "label",
                            "style": "max-width: 276px; width: 100%; position: relative; align-items: center; display: flex; transition: 150ms;",
                            "extend": [
                              {
                                "extension": ":has(input:nth-of-type(1):checked) > div:nth-of-type(1) > div:nth-of-type(1)",
                                "style": "display: block;"
                              },
                              {
                                "extension": ":has(input:nth-of-type(1):checked)",
                                "style": "border-color: D87D4A; opacity: 100;"
                              }
                            ],
                            "attributes": {
                              "class": "input-checkout"
                            },
                            "children": [
                              {
                                "element": "div",
                                "style": "position: absolute; width: 20px; height: 20px; border: 1px solid #CFCFCF; box-sizing: border-box; border-radius: 50%; left: 16px; display: flex; align-items: center; justify-content: center;",
                                "children": [
                                  {
                                    "element": "div",
                                    "style": "width: 10px; height: 10px; background-color: #D87D4A; border-radius: 50%; display: none; transition: 150ms;"
                                  }
                                ]
                              },
                              {
                                "element": "span",
                                "style": "position: absolute; left: 52px;",
                                "text": "cwrapArray[e-Money,Cash on Delivery]"
                              },
                              {
                                "element": "input",
                                "style": "position: absolute; right: 0; opacity: 0;",
                                "attributes": {
                                  "type": "radio",
                                  "tabindex": "0",
                                  "checked": "cwrapArray[,cwrapOmit]",
                                  "name": "payment",
                                  "id": "cwrapArray[e-money,cash-on-delivery]",
                                  "placeholder": "cwrapArray[Alexei Ward,alexei@mail.com]"
                                }
                              }
                            ]
                          }
                        ]
                      }
                    },
                    {
                      "element": "ul",
                      "attributes": {
                        "id": "e-money-payment-type"
                      },
                      "style": "margin: 24px 0 0 0; padding: 0; display: flex; gap: 24px 16px;",
                      "blueprint": {
                        "element": "li",
                        "style": "display: flex; flex-direction: column; gap: 9px; width: 100%;",
                        "count": 2,
                        "children": [
                          {
                            "element": "label",
                            "style": "box-sizing: border-box;",
                            "text": "cwrapArray[e-Money Number,e-Money PIN]",
                            "attributes": {
                              "for": "cwrapArray[e-money-number,e-money-pin]",
                              "class": "label1"
                            }
                          },
                          {
                            "element": "input",
                            "attributes": {
                              "class": "input-checkout",
                              "id": "cwrapArray[e-money-number,e-money-pin]",
                              "autocomplete": "off",
                              "placeholder": "cwrapArray[238521993,6891]",
                              "required": ""
                            }
                          }
                        ]
                      }
                    },
                    {
                      "element": "div",
                      "attributes": {
                        "id": "cash-on-delivery-information"
                      },
                      "style": "display: flex; align-items: center; gap: 32px; display: none; margin-top: 31px;",
                      "children": [
                        {
                          "element": "img",
                          "attributes": {
                            "src": "../static/images/checkout/icon-cash-on-delivery.svg"
                          }
                        },
                        {
                          "element": "span",
                          "style": "color: rgba(0,0,0,0.5); font-weight: 500; font-size: 15px; line-height: 25px;",
                          "text": "The ‘Cash on Delivery’ option enables you to pay in cash when our delivery courier arrives at your residence. Just make sure your address is correct so that your order will not be cancelled."
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "element": "div",
              "style": "display: flex; flex-direction: column; max-width: 350px; width: 100%; padding: 32px 33px; background-color: white; height: fit-content; border-radius: 8px; box-sizing: border-box;",
              "children": [
                {
                  "element": "h2",
                  "attributes": {
                    "class": "h2-cart"
                  },
                  "text": "SUMMARY"
                },
                {
                  "element": "ul",
                  "attributes": {
                    "id": "checkout-summary-container"
                  },
                  "style": "width: 100%; margin: 31px 0 0 0; padding: 0; list-style: none; display: flex; flex-direction: column; gap: 24px;"
                },
                {
                  "element": "section",
                  "style": "margin-top: 32px; display: flex; flex-direction: column; gap: 8px;",
                  "children": [
                    {
                      "element": "div",
                      "attributes": {
                        "class": "checkout-summary-container-div"
                      },
                      "children": [
                        {
                          "element": "h3",
                          "attributes": {
                            "class": "checkout-summary-container-h3"
                          },
                          "text": "TOTAL"
                        },
                        {
                          "element": "p",
                          "attributes": {
                            "id": "checkout-summary-total",
                            "class": "checkout-summary-container-h3-p"
                          }
                        }
                      ]
                    },
                    {
                      "element": "div",
                      "attributes": {
                        "class": "checkout-summary-container-div"
                      },
                      "children": [
                        {
                          "element": "h3",
                          "attributes": {
                            "class": "checkout-summary-container-h3"
                          },
                          "text": "SHIPPING"
                        },
                        {
                          "element": "p",
                          "attributes": {
                            "id": "checkout-summary-shipping",
                            "class": "checkout-summary-container-h3-p"
                          }
                        }
                      ]
                    },
                    {
                      "element": "div",
                      "attributes": {
                        "class": "checkout-summary-container-div"
                      },
                      "children": [
                        {
                          "element": "h3",
                          "attributes": {
                            "class": "checkout-summary-container-h3"
                          },
                          "text": "VAT (INCLUDED)"
                        },
                        {
                          "element": "p",
                          "attributes": {
                            "id": "checkout-summary-vat",
                            "class": "checkout-summary-container-h3-p"
                          }
                        }
                      ]
                    }
                  ]
                },
                {
                  "element": "div",
                  "attributes": {
                    "class": "checkout-summary-container-div",
                    "style": "margin-top: 24px;"
                  },
                  "children": [
                    {
                      "element": "h3",
                      "attributes": {
                        "class": "checkout-summary-container-h3"
                      },
                      "text": "GRAND TOTAL"
                    },
                    {
                      "element": "p",
                      "style": "color: #D87D4A;",
                      "attributes": {
                        "id": "checkout-summary-grand",
                        "class": "checkout-summary-container-h3-p"
                      }
                    }
                  ]
                },
                {
                  "element": "button",
                  "attributes": {
                    "class": "button1"
                  },
                  "extend": [
                    {
                      "extension": ":hover",
                      "style": "background-color: #FBAF85;"
                    }
                  ],
                  "style": "margin-top: 32px; max-width: 100%; background-color: #D87D4A; font-family: Manrope; transition: 150ms; cursor: pointer;",
                  "text": "CONTINUE & PAY"
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
